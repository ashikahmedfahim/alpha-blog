<h1>
  Articles
</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td>
          <%= article.title %>
        </td>
        <td>
          <%= article.description %>
        </td>
        <td>
          please add the link to edit and delete
        </td>
      </tr>
      <% end %>
  </tbody>
</table>